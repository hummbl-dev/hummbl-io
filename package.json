{
    "name": "hummbl-io",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "test:perf": "tsx scripts/testWatcherLoad.ts",
        "test:auth": "tsx scripts/testAuthPerf.ts",
        "test:ci": "pnpm test && pnpm test:perf",
        "test:ci:auth": "pnpm test && pnpm test:auth",
        "test:e2e": "jest src/__tests__/app.e2e.test.tsx --config=jest.config.js --runInBand --detectOpenHandles --verbose",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "phase2:complete": "node ./scripts/phase2-complete.mjs",
        "validate:models": "node ./scripts/validate-models.mjs",
        "audit": "node ./scripts/audit.mjs",
        "telemetry": "node ./scripts/telemetry.mjs"
    },
    "dependencies": {
        "@supabase/supabase-js": "^2.75.1",
        "@types/react": "^18.2.43",
        "@types/react-dom": "^18.2.17",
        "@vitejs/plugin-react": "^4.2.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "typescript": "^5.2.2",
        "vite": "^5.0.8"
    },
    "devDependencies": {
        "@babel/core": "^7.23.5",
        "@babel/preset-env": "^7.23.5",
        "@babel/preset-react": "^7.23.3",
        "@babel/preset-typescript": "^7.23.3",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^14.3.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/chokidar": "^1.7.5",
        "@types/commander": "^2.12.0",
        "@types/csv-stringify": "^1.4.3",
        "@types/table": "^6.0.0",
        "@vitest/coverage-v8": "^3.2.4",
        "@vitest/ui": "^3.2.4",
        "ajv": "^8.17.1",
        "axe-core": "^4.9.1",
        "chrome-launcher": "^1.1.1",
        "commander": "^14.0.1",
        "csv-stringify": "^6.6.0",
        "happy-dom": "^20.0.5",
        "jest-environment-jsdom": "^29.7.0",
        "jsdom": "^27.0.0",
        "lighthouse": "^12.2.0",
        "puppeteer": "^23.7.0",
        "table": "^6.9.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "tsx": "^4.20.6",
        "vitest": "^3.2.4"
    },
    "jest": {
        "testEnvironment": "jsdom",
        "setupFilesAfterEnv": [
            "<rootDir>/jest.setup.js"
        ],
        "moduleNameMapper": {
            "\\.(css|less|scss|sass)$": "identity-obj-proxy"
        },
        "transform": {
            "^.+\\.(ts|tsx)$": [
                "babel-jest"
            ]
        },
        "testMatch": [
            "**/__tests__/**/*.test.(ts|tsx|js|jsx)",
            "**/?(*.)+(spec|test).(ts|tsx|js|jsx)"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "engines": {
        "node": "20.x"
    }
}
