# Staging Environment Configuration
TELEMETRY_DB_PATH=./data/telemetry/events.db
STAGING_API_URL=http://localhost:3000

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=hummbl_staging
DB_USER=hummbl_user

# Security
NODE_ENV=staging
JWT_SECRET=your_jwt_secret_here

# CORS
CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=debug

# Telemetry
TELEMETRY_ENABLED=true
TELEMETRY_SAMPLE_RATE=1.0

# Timeouts
VALIDATION_TIMEOUT_MS=5000

# Feature Flags
FEATURE_NEW_VALIDATION=true
FEATURE_DEPRECATION_WARNINGS=true
